{
  "expo": {
    "name": "Comfy Portal",
    "slug": "comfy-portal",
    "version": "1.0.7",
    "owner": "sillyl12324",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "comfyportal",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "isTabletOnly": false,
      "requireFullScreen": false,
      "bundleIdentifier": "com.sillyl12324.comfy-portal",
      "appleTeamId": "QUVB58SSQ7",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSPasteboardUsageDescription": "Allow $(PRODUCT_NAME) to access your clipboard to automatically detect and add server URLs"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sillyl12324.comfyportal",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "compileSdkVersion": 36,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "networkInspector": false
          },
          "ios": {
            "deploymentTarget": "15.1",
            "networkInspector": false
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to set preview images"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to set preview images",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save generated images to your photos",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-video",
      "expo-font",
      "expo-image",
      [
        "expo-sharing",
        {
          "ios": {
            "enabled": true,
            "appGroupId": "group.com.sillyl12324.comfy-portal",
            "activationRule": {
              "supportsImageWithMaxCount": 1
            }
          }
        }
      ],
      "expo-web-browser",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "comfy-portal",
          "organization": "lotusfino-inc"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "841218f8-f7aa-4f62-8687-d1e2abaa3e5d",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "expo-sharing-extension",
                  "bundleIdentifier": "com.sillyl12324.comfy-portal.expo-sharing-extension",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.sillyl12324.comfy-portal"
                    ]
                  }
                }
              ]
            }
          }
        }
      }
    }
  }
}
